<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Nfc_test" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="android:background">@color/white</item>
        <item name="android:windowActionBar" tools:targetApi="honeycomb">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimary">@color/teal_700</item>
        <item name="colorPrimaryVariant">@color/teal_700</item>
        <item name="colorOnPrimary">#C9574F4F</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#1976D2</item>
        <item name="colorSecondaryVariant">#0288D1</item>
        <item name="colorOnSecondary">#C9574F4F</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:textColor">?attr/colorPrimaryVariant</item>
        <item name="isMaterialTheme">true</item>
    </style>
</resources>